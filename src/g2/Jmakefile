G2_SRC = \
	packet.c \
	udp_transceiver.c 

G2_OBJ = \
|expand f!$(G2_SRC)!
!f:\.c=.o \
-expand \\

++GTK_CFLAGS $gtkcflags
++GTK_LDFLAGS $gtkldflags

CFLAGS = -I$(TOP) $(GTK_CFLAGS) $(XML2_CFLAGS)
DPFLAGS = -I$(TOP) $(GTK_CFLAGS) $(XML2_CFLAGS)
LDFLAGS = $(GTK_LDFLAGS) $(XML2_LDFLAGS)

NormalLibraryTarget(g2, $(G2_SRC), $(G2_OBJ))

